<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Search Results</title>
</head>
<body>
    <input type="search" id="search" name="search" required>
    <h1>Search Results</h1>
    <ul id="list" class="list"></ul>
        <% clans.forEach(clan => { %>
            <li>
                <h2><%= clan.name %> (#<%= clan.tag %>)</h2>
                <p>Level: <%= clan.clanLevel %></p>
                <p>Members: <%= clan.members %></p>
                <p>Description: <%= clan.description %></p>
            </li>
        <% }) %>
    </ul>
</body>
<script src="https://cdn.jsdelivr.net/npm/list.js@2.3.1/dist/list.min.js"></script>
<script>
    var options = {
        valueNames: ['name', 'tag', 'clanLevel', 'members', 'description']
    };
    var clanList = new List('list', options);
</script>

</html>